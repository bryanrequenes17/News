<div class="container">
    <h1><%= h1 %></h1>
    <blockquote>
        <%= blokq %>
    </blockquote>
    <div class="row">
        <form class="col s12" method="post" action="/crearedicion" id="validation-form">
            <div class="row">
                <div class="input-field col s6">
                    <input placeholder="" name="edicion" id="edicion" type="text" class="validate" required>
                    <label for="Edicion">Edicion</label>
                </div>
                <div class="input-field col s6">
                    <input id="fecha" name="fecha" type="date" class="validate" required>
                    <label for="fecha">Fecha de Publicacion</label>
                </div>
            </div>
            <button class="btn waves-effect waves-light" type="submit" name="action">Crear
                <i class="material-icons right">send</i>
            </button>
        </form>
    </div>
</div>

